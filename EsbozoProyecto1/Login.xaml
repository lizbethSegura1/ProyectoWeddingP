<Window x:Class="EsbozoProyecto1.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EsbozoProyecto1"
        mc:Ignorable="d"
        Title="Login" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" Content="EMAIL" />
        <TextBox Grid.Row="0" Name="Email" Margin="62,9.4,21.6,0.4"/>
        <Label Grid.Row="1" Content="PASSWD" />
        <TextBox Grid.Row="1" Name="Passwd" Margin="62,4.6,21.6,5.2"/>
        <Button Grid.Row="2" Content="LOGIN" Margin="5,4.8,452.6,5" Click="OnLogin"/>
        <Button Grid.Row="2" Content="CREATE USER" Margin="355,4.8,21.6,5" Click="OnCreateUser"/>
    </Grid>
</Window>
